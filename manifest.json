{
  "name": "LinkedInScrapingExtension",
  "description": "Scrap LinkedIn profiles",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/*"],
      "js": ["./app.js"]
    }
  ],
  "action":{
    "default_popup":"./App/index.html",
    "default_icon": {
      "32": "./App/Image/32.png",
      "64": "./App/Image/64.png"
    }
  },
  "permissions": ["activeTab", "scripting"],
  "icons": {
    "32": "./App/Image/32.png",
    "64": "./App/Image/64.png"
  }
}